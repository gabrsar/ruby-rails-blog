<h1> Olha quando artigo lindo </h1>


<%= link_to 'New artiKle', new_article_path %>
<table>
	<tr>
		<th>Title</th>
		<th>Text</th>
	</tr>
	<% @articles.each do |article| %>
		<tr>
			<td><%= article.title %></td>
			<td><%= article.text %></td>
			<td><%= link_to 'SHOW', article_path(article) %></td>
		</tr>
	<% end %>
</table>

